---
import { Bars3Icon } from '@heroicons/react/24/outline';
export interface Props {
    projectName: string;
    pages: {
        title: string,
        url: string
    }[]
}

const { projectName, pages } = Astro.props;
---


<div class="bg-secondary text-white w-full h-[80px] px-8 py-6 flex flex-row items-center gap-[52px]">
    <Bars3Icon className="w-8 h-8" />
    <span class="text-lg">{ projectName }</span>
</div>