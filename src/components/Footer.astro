---
export interface Props {
    projectBy: string;
}

const { projectBy } = Astro.props;
---

<div class="h-[80px] bg-secondary w-full px-8 py-6 flex flex-row items-center justify-between text-white">
    <div class="flex flex-row gap-2">
        <span>Project By:</span>
        <span class="font-semibold">{ projectBy }</span>
    </div>
    <div>
        This site was generated by <span class="font-semibold">AVAnnotate</span>
    </div>
</div>